{
    "name": "ghazniali95/shopifyconnector",
    "description": "Connects and Performs operations for shopify.",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Ghazniali95\\ShopifyConnector\\": "src/"
        }
    },
    "keywords": [
        "shopify",
        "node",
        "jwt",
        "app",
        "graphql",
        "rest",
        "webhook",
        "Admin API",
        "Storefront API"
    ],
    "authors": [
        {
            "name": "Ghazni Ali",
            "email": "ghazniali95@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "Ghazniali95\\ShopifyConnector\\App\\Providers\\ShopifyConnectorProvider"
            ],
            "aliases": {
                "ShopifyService": "Ghazniali95\\ShopifyConnector\\App\\Facades\\ShopifyService"
            }
        }
    }, 
    "require": {
        "php": "~8.0.0 || ~8.1.0 || ~8.2.0",
        "phpunit/phpunit": "^9.0",
        "ext-ctype": "*",
        "ext-hash": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "doctrine/inflector": "^2.0",
        "firebase/php-jwt": "^5.2 || ^6.2",
        "guzzlehttp/guzzle": "^7.4.4",
        "guzzlehttp/psr7": "^2.0",
        "psr/http-client": "^1.0",
        "psr/http-message": "^1.0",
        "psr/log": "^1.1 || ^2.0 || ^3.0",
        "ramsey/uuid": "^4.1"
    },
    "require-dev": {
        "ergebnis/composer-normalize": "^2.30",
        "maglnet/composer-require-checker": "^3.0 || ^4.0",
        "mikey179/vfsstream": "^1.6",
        "phpunit/phpunit": "^9.0",
        "squizlabs/php_codesniffer": "^3.6" 
    },
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true
        }
    }


}
